        function exportData() {
            try {
                const data = JSON.parse(localStorage.getItem('coc_login_data') || '[]');
                
                if (data.length === 0) {
                    alert('Ù„Ø§ ØªÙˆØ¬Ø¯ Ø¨ÙŠØ§Ù†Ø§Øª Ù„Ù„ØªØµØ¯ÙŠØ±');
                    return;
